{
  "name": "cenotes",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "serve": "vite --port 8080",
    "build": "vue-tsc -b && vite build",
    "deploy": "npm run update && vite build --base=/CeNotes/",
    "update": "node ./scripts/generateSonglist.js",
    "preview": "vite preview --port 8080"
  },
  "dependencies": {
    "midi-json-parser": "^8.1.56",
    "opensheetmusicdisplay": "^1.8.9",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.0",
    "typescript": "~5.6.3",
    "vite": "^5.4.11",
    "vite-plugin-pwa": "^0.21.0",
    "vue-tsc": "^2.1.10"
  }
}
